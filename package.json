{
  "name": "vote-counter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "build:check": "tsc --noEmit --skipLibCheck --project tsconfig.prod.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest --config jest.config.js",
    "test:e2e": "playwright test",
    "test:e2e:cli": "cross-env PW_TEST_HTML_REPORT_OPEN=never playwright test",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:cli:chrome": "cross-env PW_TEST_HTML_REPORT_OPEN=never playwright test --project=chromium"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hotkeys-hook": "^4.6.2",
    "recharts": "^2.15.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.50.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "vite": "^7.1.0"
  }
}
